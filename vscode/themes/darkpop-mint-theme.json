    "tokenColors": [
        {
            "scope": "keyword.control, keyword, storage.modifier, keyword.operator",
            "settings": {
                "foreground": "#FF476E",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string.quoted, string",
            "settings": {
                "foreground": "#94bfff"
            }
        },
        {
            "scope": "constant.numeric, number",
            "settings": {
                "foreground": "#f1b3f1"
            }
        },
        {
            "scope": "entity.name.function.preprocessor",
            "settings": {
                "foreground": "#f1b3f1",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "comment, comment.line, comment.block",
            "settings": {
                "foreground": "#919191",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "comment.documentation, comment.block.documentation",
            "settings": {
                "foreground": "#95FF6E"
            }
        },
        {
            "scope": "storage.type, meta.declaration, entity.name.type.class, storage.modifier",
            "settings": {
                "foreground": "#FF476E",
                "fontStyle": "italic bold"
            }
        },
        {
            "scope": "meta.decorator, storage.modifier, meta.annotation, keyword.other.decorator, entity.name.function.decorator",
            "settings": {
                "foreground": "#ededed",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "punctuation.operator, entity.name.function.operator",
            "settings": {
                "foreground": "#ededed"
            }
        },
        {
            "scope": "meta.function entity.name, support.function.constructor, entity.name.function.definition, meta.head.function.definition",
            "settings": {
                "foreground": "#01f7f7",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "meta.function-call, entity.name.function.call, entity.name.function.member",
            "settings": {
                "foreground": "#ffffff",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "meta.block.class.cpp entity.name.type.class.cpp, entity.name.type.class.cpp, entity.name.type.class, entity.name.class",
            "settings": {
                "foreground": "#01f7f7",
                "fontStyle": "italic bold"
            }
        },
        {
            "scope": "variable.other, meta.definition.variable, identifier, meta.function-call.arguments",
            "settings": {
                "foreground": "#ededed"
            }
        },
        {
            "scope": "variable.language.self, variable.language.this, variable.language.special.self, variable.language.special.this, variable.parameter.function.language.special.self, variable.parameter.function.language.special.this",
            "settings": {
                "foreground": "#ffc232",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "constant.language, keyword.constant",
            "settings": {
                "foreground": "#FF476E",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "support.type.builtin, entity.name.type.builtin, storage.type.builtin, support.function.builtin, support.type",
            "settings": {
                "foreground": "#63ff99"
            }
        },
        {
            "scope": "source",
            "settings": {
                "foreground": "#ffffff"
            }
        }
    ]
}